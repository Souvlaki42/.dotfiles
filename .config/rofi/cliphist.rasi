configuration {
    modi: "clipboard:~/cliphist-rofi-img";
    icon-theme: "Papirus";
    font: "JetBrainsMono Nerd Font Medium 10";
    show-icons: true;
    terminal: "alacritty";
    location: 0;
    disable-history: false;
    hide-scrollbar: false;
    sidebar-mode: false;
    display-clipboard: "Clipboard";
}

@theme "catppuccin-mocha"

#window {
  border-radius: 20px;
  border-color: @blue;
}

#listview {
  columns: 1;
  lines: 6;
}
